{
    "manifest_version": 2,

    "name": "ban-shopping",
    "description": "",
    "version": "0.1",

    "browser_action": {
        "default_icon": "te.png",
        "default_popup": "popup.html"
    },

    "content_scripts": [
        {
            "matches": [
                "*://*.jd.com/*",
                "*://*.taobao.com/*"
            ],
            "js": ["ban-shopping.js"]
        }
    ]
}